SELECT
    RunAt,
    Cutoff,
    COUNT(*)                    AS DeletedCount,
    MIN(TelemetryTS)            AS OldestDeletedTS,   -- 一番古く消したレコードのTS
    MAX(TelemetryTS)            AS NewestDeletedTS    -- 一番新しく消したレコードのTS
FROM PlantDB.dbo.PurgeLogDetail
GROUP BY RunAt, Cutoff
ORDER BY RunAt DESC;

